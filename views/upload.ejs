<body></body>
    <iframe src="https://lottie.host/embed/7d285896-0895-477c-8b5d-b8093591a54a/UJWfTxLIny.json" id="buffer" style="width: 100%; height: 100%;"></iframe>
    <div class="container">
        
        <form id="form" aria-disabled="true">
            <nav class="navbar bg-body-tertiary ">
                <div class="container-fluid">
                    <a class="navbar-brand" href="#" style="display: flex;justify-content: center;align-items: center;">
                        <img src="/images/adaptive-icon.png" alt="Logo" width="70" height="70"
                            class="d-inline-block align-text-top">
                        ProctorCalling
                    </a>
                </div>
            </nav>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Full name</label>
                <input type="text" class="form-control form" id="name" aria-describedby="emailHelp">
            </div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">PRN number</label>
                <input type="text" class="form-control form" id="prlNumber" aria-describedby="emailHelp">
            </div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Roll number</label>
                <input type="text" class="form-control form" id="rollNumber" aria-describedby="emailHelp">
            </div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Parent contact Number</label>
                <input type="text" class="form-control form" id="parentNumber" aria-describedby="emailHelp">
            </div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Your contact Number</label>
                <input type="text" class="form-control form" id="studentNumber" aria-describedby="emailHelp">
            </div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Division</label>
                <select class="form-select form" aria-label="Default select example" id="division">
                    <option value="A">A</option>
                    <option value="B">B</option>
                    <option value="C">C</option>
                    <option value="D">D</option>
                    <option value="E">E</option>
                    <option value="F">F</option>
                    <option value="G">G</option>
                    <option value="H">H</option>
                </select>
            </div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Department</label>
                <select class="form-select form" aria-label="Default select example" id="department">
                    <option value="CSE">Computer science and engineering</option>
                    <option value="EE">Electrical Engineering</option>
                    <option value="AIDS">Artificial Intelligent and Data science
                    </option>
                    <option value="MECH">Mechanical</option>
                    <option value="CIVIL">Civil</option>
                </select>
            </div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Year</label>
                <select class="form-select form" aria-label="Default select example" id="Class">
                    <option value="FY">First Year</option>
                    <option value="SY">Second Year</option>
                    <option value="TY">Third Year</option>
                    <option value="LY">Last Year</option>
                </select>
            </div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Email</label>
                <input type="email" class="form-control form" id="email" aria-describedby="emailHelp">
            </div>
            <div style="display: flex; align-items: flex-end;">
                <div class="mb-3" style="flex-grow: 1;">
                    <label for="exampleInputEmail1" class="form-label">Password</label>
                    <input type="password" class="form-control form" id="password" aria-describedby="emailHelp">
                </div>
                <button type="button" id="passwordShow" onclick="passwordShowfunction()"
                    style="height: 40px; margin-bottom: 15px; margin-left: 10px;display: flex;justify-content: center;align-items: center;background-color: transparent;border: 0px solid transparent;">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                        class="bi bi-eye-slash" viewBox="0 0 16 16">
                        <path
                            d="M13.359 11.238C15.06 9.72 16 8 16 8s-3-5.5-8-5.5a7 7 0 0 0-2.79.588l.77.771A6 6 0 0 1 8 3.5c2.12 0 3.879 1.168 5.168 2.457A13 13 0 0 1 14.828 8q-.086.13-.195.288c-.335.48-.83 1.12-1.465 1.755q-.247.248-.517.486z" />
                        <path
                            d="M11.297 9.176a3.5 3.5 0 0 0-4.474-4.474l.823.823a2.5 2.5 0 0 1 2.829 2.829zm-2.943 1.299.822.822a3.5 3.5 0 0 1-4.474-4.474l.823.823a2.5 2.5 0 0 0 2.829 2.829" />
                        <path
                            d="M3.35 5.47q-.27.24-.518.487A13 13 0 0 0 1.172 8l.195.288c.335.48.83 1.12 1.465 1.755C4.121 11.332 5.881 12.5 8 12.5c.716 0 1.39-.133 2.02-.36l.77.772A7 7 0 0 1 8 13.5C3 13.5 0 8 0 8s.939-1.721 2.641-3.238l.708.709zm10.296 8.884-12-12 .708-.708 12 12z" />
                    </svg>
                </button>

               
            </div>
            <input type="submit"  value="submit" class="btn btn-success" id="submit"
            style="height: 40px; margin-bottom: 15px; margin-left: 10px;">
            </form>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
